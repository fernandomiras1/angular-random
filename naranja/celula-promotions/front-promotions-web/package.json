{
    "name": "webapp",
    "version": "3.0.6",
    "scripts": {
        "start": "ng serve --port=4201 --proxy-config proxy.config.js",
        "start:stubs": "concurrently \"ng serve -c stubs --host 0.0.0.0 \" \"npm:stubs\"",
        "start:proxy": "ng serve -o --proxy-config proxy.config.js",
        "start:ssr": "node dist/server",
        "build": "npm-run-all clean build:front:**",
        "build:develop": "npm-run-all clean build:front:** --webapp:build=develop",
        "build:testing": "npm-run-all clean build:front:** --webapp:build=testing",
        "build:staging": "npm-run-all clean build:front:** --webapp:build=staging",
        "build:prodblue": "npm-run-all clean build:front:** --webapp:build=prodblue",
        "build:front:client:ng": "ng build --configuration=${npm_package_config_build}",
        "build:front:server:ng": "ng run webapp:server:${npm_package_config_build}",
        "build:front:client:pwa": "node workbox-build-inject.js",
        "build:front:client:copy": "cp LICENSE dist/promociones",
        "build:front:server": "webpack --config webpack.server.config.js --progress --colors",
        "test": "ng test --browsers=Chrome",
        "test:build": "rimraf ./coverage && ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
        "lint": "npm-run-all -p lint:*",
        "lint:app": "tslint \"src/**/*.ts\" --project tsconfig.json --fix -t verbose",
        "lint:scss": "stylelint \"src/**/*.scss\" --syntax scss",
        "serve:ssr": "node dist/server",
        "clean": "rimraf dist",
        "serve:contentful": "node contentful.js | pino-pretty",
        "report": "ng build -c prodblue --stats-json && webpack-bundle-analyzer dist/promociones/stats.json"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "1.0.0",
        "@agm/js-marker-clusterer": "1.0.0",
        "@agm/snazzy-info-window": "1.0.0",
        "@angular/animations": "~7.2.16",
        "@angular/common": "~7.2.16",
        "@angular/compiler": "~7.2.16",
        "@angular/core": "~7.2.16",
        "@angular/forms": "~7.2.16",
        "@angular/http": "~7.2.16",
        "@angular/platform-browser": "~7.2.16",
        "@angular/platform-browser-dynamic": "~7.2.16",
        "@angular/platform-server": "~7.2.16",
        "@angular/router": "~7.2.16",
        "@ngrx/effects": "^7.4.0",
        "@ngrx/router-store": "^7.4.0",
        "@ngrx/store": "^7.4.0",
        "@ngrx/store-devtools": "^7.4.0",
        "@nguniversal/common": "^7.1.1",
        "@nguniversal/express-engine": "^7.1.1",
        "@nguniversal/module-map-ngfactory-loader": "^0.0.0",
        "@ngx-translate/core": "^11.0.1",
        "@ngx-translate/http-loader": "^4.0.0",
        "@zumo/core": "5.0.1-beta.0",
        "@zumo/ncom-footer": "0.17.0",
        "@zumo/zumo-snackbar": "1.0.0",
        "@zumo/zumo-utils": "1.0.0",
        "body-scroll-lock": "^3.0.1",
        "bundle-loader": "^0.5.6",
        "compression": "^1.7.4",
        "contentful": "^7.0.5",
        "contentful-text-search": "^0.1.1",
        "core-js": "^2.5.4",
        "crypto-js": "^3.1.9-1",
        "domino": "^2.1.3",
        "express": "^4.16.4",
        "hammerjs": "^2.0.8",
        "js-marker-clusterer": "^1.0.0",
        "lazysizes": "^5.1.0",
        "lodash-es": "^4.17.15",
        "luxon": "^1.24.0",
        "memory-cache": "^0.2.0",
        "rxjs": "~6.3.3",
        "snazzy-info-window": "^1.1.1",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular-devkit/schematics": "^7.3.8",
        "@angular/cli": "~7.3.8",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@ngrx/schematics": "^7.4.0",
        "@types/googlemaps": "^3.38.0",
        "@types/hammerjs": "^2.0.36",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "concurrently": "^4.1.0",
        "cpy-cli": "^2.0.0",
        "cross-env": "^7.0.2",
        "husky": "^1.3.1",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-spec-reporter": "0.0.32",
        "lighthouse-ci": "^1.10.0",
        "npm-run-all": "^4.1.5",
        "pino-pretty": "^2.5.0",
        "protractor": "~5.4.0",
        "puppeteer": "^1.16.0",
        "rimraf": "^2.6.3",
        "stylelint-config-suitcss": "^14.0.0",
        "stylelint-selector-bem-pattern": "^2.1.0",
        "ts-loader": "^5.2.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "tslint-config-airbnb": "^5.11.1",
        "typescript": "~3.2.2",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-cli": "^3.1.0",
        "workbox-build": "^4.3.1"
    }
}